- survey.attributes.each do |k,v|
  - if k[/section/]
    = "Section #{k.split('_')[1]} Number #{k.split('_')[2]}: #{v}"
    %br
- Efficacy.find_by_part2_id(survey.id).attributes.each do |k,v|
  - if k[/efficacy/]
    = "Efficacy #{k.split('_')[1]}: #{v}"
    %br
= button_to "Edit Pre-Survey 2", edit_presurveys_part2_path(survey), :method => :get, :id => "edit_pre_survey_part2_#{survey.id}"
